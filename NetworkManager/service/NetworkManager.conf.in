connectivity.add("connMonitorEnpt4", "@PLUGIN_NETWORKMANAGER_CONN_MONITOR_ENDPOINT_4@")
connectivity.add("connMonitorEnpt5", "@PLUGIN_NETWORKMANAGER_CONN_MONITOR_ENDPOINT_5@")
